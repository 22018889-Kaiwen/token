    "rotating-file-stream": "^3.2.1",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "unplugin-swc": "^1.4.5",
    "vitest": "^1.5.3",
    "web3": "^4.8.0",
    "web3-validator": "^2.0.6",
    "zod": "^3.23.5"
  },
